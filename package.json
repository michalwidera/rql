{
  "name": "rql",
  "displayName": "RQL Code HighLight",
  "description": "Syntax Highlight for RQL Queries",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.81.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "rql",
      "aliases": ["rql", "rql"],
      "extensions": [".rql",".desc"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "rql",
      "scopeName": "source.rql",
      "path": "./syntaxes/rql.tmLanaguage"
    }]
  }
}
